@article{hosseinpoor_social_2011,
	title = {Social determinants of smoking in low- and middle-income countries: {Results} from the world health survey},
	volume = {6},
	issn = {19326203},
	doi = {10.1371/journal.pone.0020331},
	abstract = {INTRODUCTION: Tobacco smoking is a leading cause of premature death and disability, and over 80\% of the world's smokers live in low- or middle-income countries. The objective of this study is to assess demographic and socioeconomic determinants of current smoking in low- and middle-income countries.\${\textbackslash}backslash\$n\${\textbackslash}backslash\$nMETHODS: We used data, from the World Health Survey in 48 low-income and middle-income countries, to explore the impact of demographic and socioeconomic factors on the current smoking status of respondents. The data from these surveys provided information on 213,807 respondents aged 18 years or above that were divided into 4 pooled datasets according to their sex and country income group. The overall proportion of current smokers, as well as the proportion by each relevant demographic and socioeconomic determinant, was calculated within each of the pooled datasets, and multivariable logistic regression was used to assess the association between current smoking and these determinants.\${\textbackslash}backslash\$n\${\textbackslash}backslash\$nRESULTS: The odds of smoking were not equal in all demographic or socioeconomic groups. Some factors were fairly stable across the four datasets studied: for example, individuals were more likely to smoke if they had little or no education, regardless of if they were male or female, or lived in a low or a middle income country. Nevertheless, other factors, notably age and wealth, showed a differential effect on smoking by sex or country income level. While women in the low-income country group were twice as likely to smoke if they were in the lowest wealth quintile compared with the highest, the association was absent in the middle-income country group.\${\textbackslash}backslash\$n\${\textbackslash}backslash\$nCONCLUSION: Information on how smoking is distributed among low- or middle-income countries will allow policy makers to tailor future policies, and target the most vulnerable populations.},
	number = {5},
	journal = {PLoS ONE},
	author = {Hosseinpoor, Ahmad Reza and Parker, Lucy Anne and Tursan d'Espaignet, Edouard and Chatterji, Somnath},
	year = {2011},
	pmid = {21655299}
}

