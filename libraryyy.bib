  keywords = {Aged, Alcoholic Beverages, Alcoholic Beverages: economics, Alcoholic Beverages: poisoning, Alcoholic Beverages: supply \& distribution, Alcoholism, Alcoholism: epidemiology, Automobile Driving, Automobile Driving: statistics \& numerical data, Denmark, Humans, Middle Aged, Models, Statistical, Sweden, Sweden: epidemiology, Taxes, Violence, Violence: statistics \& numerical data},
