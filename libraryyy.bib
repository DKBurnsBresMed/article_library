@article{malesky_foreign_2009,
	title = {Foreign {Direct} {Investors} as {Agents} of {Economic} {Transition}: {An} {Instrumental} {Variables} {Analysis}},
	volume = {4},
	issn = {15540634},
	doi = {10.1561/100.00008068},
	abstract = {Previous empirical analysis has noted a correlation between Foreign\${\textbackslash}backslash\$nDirect Investment (FDI) and economic reform in Eastern Europe and\${\textbackslash}backslash\$nthe Former Soviet Union, but has attributed the relationship to investors\${\textbackslash}backslash\$nrewarding countries after reform decisions. Little attention has\${\textbackslash}backslash\$nbeen paid to the fact that investors' lobbying efforts may actually\${\textbackslash}backslash\$ninfluence reform choices. This paper finds a positive effect of FDI\${\textbackslash}backslash\$non reform progress through a panel analysis of investor influence\${\textbackslash}backslash\$nin 27 transition states (1991-2004). To address endogeneity bias,\${\textbackslash}backslash\$nthe exogenous portion of a country's exchange rate movement is used\${\textbackslash}backslash\$nas an instrument in a two-stage procedure. The underlying counterfactual\${\textbackslash}backslash\$ncomparison that results from this approach is between two similarly\${\textbackslash}backslash\$nsituated countries, but where one country experienced a large shift\${\textbackslash}backslash\$nin the share of FDI in its economy as a result of changes in the\${\textbackslash}backslash\$ninternational economy and the other did not. Further analysis reveals\${\textbackslash}backslash\$nthat the relationship is particularly strong in the manufacturing\${\textbackslash}backslash\$nand service sectors, but does not hold for construction, utilities,\${\textbackslash}backslash\$nor natural resource based projects.},
	number = {1},
	journal = {Quarterly Journal of Political Science},
	author = {Malesky, Edmund J.},
	year = {2009},
	pages = {59--85}
}

@article{stickley_social_2009,
	title = {The social and economic determinants of smoking in {Moscow}, {Russia}},
	volume = {37},
	issn = {1403-4948, 1651-1905},
	url = {http://sjp.sagepub.com/content/37/6/632},
	doi = {10.1177/1403494809105434},
	abstract = {Background: Despite a high prevalence of smoking for decades, recent research has documented an increase in the rates of both male and female smoking in post-Soviet Russia. As yet, however, little research has taken place on smoking at the subnational level. The current study addresses this deficit by examining smoking in Moscow --- the city that has been at the forefront of the entry into the Russian market of transnational tobacco corporations (TTCs) in the transition period. Methods: Data were obtained from the Moscow Health Survey 2004 --- a stratified random sample of 1190 people representative of Moscow's larger population. Information was obtained about subjects' smoking habits and age of smoking initiation. Results: The prevalence of smoking was high among both men (55.5\%) and women (26.9\%), with significantly higher rates in the younger age groups. There was also a high prevalence of smoking initiation before age 15 years, especially in the youngest women (18---30 years). Logistic regression analysis showed that respondents' age, binge drinking, locus of control and economic situation were important determinants of smoking. Conclusions: Although lifestyle factors seem to underpin the generally high levels of smoking, other things, such as its high prevalence in the younger generations and the factors associated with smoking (locus of control), nevertheless suggest that the TTCs may have played an important role in the spread of smoking in transitional Russia's changing social environment.},
	number = {6},
	journal = {Scandinavian Journal of Public Health},
	author = {Stickley, Andrew and Carlson, Per},
	year = {2009},
	pmid = {19451199},
	keywords = {binge drinking, lifestyles, moscow, russia, smoking},
	pages = {632--639}
}

@article{guillemin_cross-cultural_1993,
	title = {Cross-cultural adaptation of health-related quality of life measures: {Literature} review and proposed guidelines},
	volume = {46},
	issn = {08954356},
	doi = {10.1016/0895-4356(93)90142-N},
	abstract = {Clinicians and researchers without a suitable health-related quality of life (HRQOL) measure in their own language have two choices: (1) to develop a new measure, or (2) to modify a measure previously validated in another language, known as a cross-cultural adaptation process. We propose a set of standardized guidelines for this process based on previous research in psychology and sociology and on published methodological frameworks. These guidelines include recommendations for obtaining semantic, idiomatic, experiential and conceptual equivalence in translation by using back-translation techniques and committee review, pre-testing techniques and re-examining the weights of scores. We applied these guidelines to 17 cross-cultural adaptation of HRQOL measures identified through a comprehensive literature review. The reporting standards varied across studies but agreement between raters in their ratings of the studies was substantial to almost perfect (weighted κ = 0.66-0.93) suggesting that the guidelines are easy to apply. Further research is necessary in order to delineate essential versus optional steps in the adaptation process.},
	journal = {Journal of Clinical Epidemiology},
	author = {Guillemin, F. and Bombardier, C. and Beaton, D.},
	year = {1993},
	pmid = {8263569},
	keywords = {Cross-cultural comparison, Culture, Guidelines, Health status index, Quality of life, Validity},
	pages = {1417--1432}
}

