@article{bradshaw_changing_2002,
	title = {The changing geography of foreign investment in the {Russian} federation},
	volume = {11},
	issn = {09670793},
	url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0036162667&partnerID=40&md5=d39c55eda1d1cb1c164c733ba7b4b74f},
	abstract = {This paper poses three main questions. Who is investing in Russia? Foreign investment is dominated by companies from the US and EU, plus countries such as Cyprus and Switzerland which represent returning Russian flight capital. Which sectors of the economic are benefiting from such investment? Initial investment flows were dominated by market-oriented investment in Moscow city and investment in the resource regions; post 1998 financial crisis, there is evidence of import substituting investment in consumer oriented sectors, most notably the food industry. Which regions are the major recipients of investment? The shift in motivation after 1998 brought with it the acceleration of a trend towards investment beyond Moscow city. That said, FDI remains highly concentrated in a relatively small number of regions. Many of those same regions also represent some of Russia's most developed regions, they are the regions at the top of the various investment ratings (such as those conducted by Ekspert Magazine (2001)) and they have also been the major recipients of Western technical assistance. Thus one can conclude that foreign investment activity, together with other forms of internationalisation, is amplifying existing regional inequalities. It is certainly the case that large parts of Russian territory remain untouched by increased interaction with the global economy.},
	number = {1},
	journal = {Russian Economic Trends},
	author = {Bradshaw, Michael J},
	year = {2002},
	keywords = {Foreign direct investment, Globalization, investment, regional pattern, Russian Federation},
	pages = {33--41}
}

