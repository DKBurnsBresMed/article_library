	keywords = {Commerce, Commerce: economics, Commerce: legislation \& jurisprudence, Commerce: organization \& administration, developing countries, Diet, Food Supply, Health Promotion, Health Promotion: organization \& administration, Humans, Internationality, Nutrition Disorders, Nutrition Disorders: epidemiology, Public Health Practice, world health},
