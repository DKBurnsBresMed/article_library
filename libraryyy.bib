@article{scott_1997_1997,
	title = {1997 index of economic freedom - {Holmes},{KR}, {Johnson},{BT}, {Kirkpatric},{M}},
	volume = {75},
	issn = {0017-8012},
	abstract = {With the failure of foreign aid and similar development projects, many analysts are saying that poor countries should lust free up their economies and let markets do the work. The Heritage Foundation's 1997 Index of Economic Freedom expands on this line of reasoning in a survey of government intervention and economic development. In his review, Harvard Business School Professor Bruce Scott contends that although some of the Inner's claims make sense, the editors have mistaken the chain of causation in many cases. Governments in South Korea and Taiwan, for example, liberalized only after their heavy-handed actions brought their economies a degree of prosperity. Similarly, Great Britain converted to a free-trade regime in the nineteenth century only after gaining commercial supremacy under protectionism. Some freedoms, such as an internal market without barriers, promote economic development, but free trade for a developing country can actually get in the way. The Index also insists that people concerned about their economic future should care only about their nation's sustainable growth. But people's priorities change as their incomes rise. As citizens become affluent, they are likely to accept the trade-off between higher growth and the security that government programs provide. They often care about social fairness and begin to press governments to address the widening gap between rich and poor. It is not clear that the magic of the marketplace can take care of everything. Citizens still need government for some things - and a broader framework of analysis than the Index for tackling economic choices.},
	language = {English},
	journal = {Harvard Business Review},
	author = {Scott, B R},
	year = {1997},
	pages = {156--\&}
}

