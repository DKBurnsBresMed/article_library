@book{odonnell_analyzing_2008,
	title = {Analyzing {Health} {Equity} {Using} {Household} {Survey} {Data}},
	isbn = {978-0-8213-6933-3},
	url = {http://scholar.google.com/scholar?hl=en&btnG=Search&q=intitle:Analyzing+Health+Equity+Using+Household+Survey+Data#5},
	abstract = {Equity has long been considered an important goal in the health sector. Yet inequalities\${\textbackslash}backslash\$nbetween the poor and the better-off persist. The poor tend to suffer higher\${\textbackslash}backslash\$nrates of mortality and morbidity than do the better-off. They often use health services\${\textbackslash}backslash\$nless, despite having higher levels of need. And, notwithstanding their lower\${\textbackslash}backslash\$nlevels of utilization, the poor often spend more on health care as a share of income\${\textbackslash}backslash\$nthan the better-off. Indeed, some nonpoor households may be made poor precisely\${\textbackslash}backslash\$nbecause of health shocks that necessitate out-of-pocket spending on health.\${\textbackslash}backslash\$nMost commentators accept that these inequalities refl ect mainly differences in\${\textbackslash}backslash\$nconstraints between the poor and the better-off - lower incomes, higher time costs,\${\textbackslash}backslash\$nless access to health insurance, living conditions that are more likely to encourage\${\textbackslash}backslash\$nthe spread of disease, and so on - rather than differences in preferences (cf. e.g.,\${\textbackslash}backslash\$nAlleyne et al. 2000; Braveman et al. 2001; Evans et al. 2001a; Le Grand 1987; Wagstaff\${\textbackslash}backslash\$n2001; Whitehead 1992). Such inequalities tend therefore to be seen not simply as\${\textbackslash}backslash\$ninequalities but as inequities (Wagstaff and van Doorslaer 2000).\${\textbackslash}backslash\$nSome commentators, including Nobel prize winners James Tobin (1970) and\${\textbackslash}backslash\$nAmartya Sen (2002), argue that inequalities in health are especially worrisome -\${\textbackslash}backslash\$nmore worrisome than inequalities in most other spheres. Health and health care\${\textbackslash}backslash\$nare integral to people's capability to function -their ability to fl ourish as human\${\textbackslash}backslash\$nbeings. As Sen puts it, "Health is among the most important conditions of human\${\textbackslash}backslash\$nlife and a critically signifi cant constituent of human capabilities which we have reason\${\textbackslash}backslash\$nto value" (Sen 2002). Society is not especially concerned that, say, ownership\${\textbackslash}backslash\$nof sports utility vehicles is low among the poor. But it is concerned that poor children\${\textbackslash}backslash\$nare systematically more likely to die before they reach their fi fth birthday and\${\textbackslash}backslash\$nthat the poor are systematically more likely to develop chronic illnesses. Inequalities\${\textbackslash}backslash\$nin out-of-pocket spending matter too, because if the poor - through no fault\${\textbackslash}backslash\$nof their own - are forced into spending large amounts of their limited incomes on\${\textbackslash}backslash\$nhealth care, they may well end up with insuffi cient resources to feed and shelter\${\textbackslash}backslash\$nthemselves.},
	number = {December 1980},
	publisher = {World Bank Publications},
	author = {O'Donnell, O and Doorslaer, E Van},
	year = {2008},
	note = {DOI: 10.2471/BLT.08.052357}
}

@article{daude_quality_2007,
	title = {The quality of institutions and foreign direct investment},
	volume = {19},
	issn = {09541985},
	doi = {10.1111/j.1468-0343.2007.00318.x},
	abstract = {Using bilateral foreign direct investment (FDI) stocks around the world, we explore the importance of a wide range of institutional variables as determinants of the location of FDI. While we find that better institu- tions have overall a positive and economically significant effect on FDI, some institutional aspects matter more than others do. Especially, the unpredictability of laws, regulations and policies, excessive regulatory burden, government instability and lack of commitment play a major role in deterring FDI. For example, the effect of a one standard devia- tion improvement in the regulatory quality of the host country increases FDI by a factor of around 2. These results are robust to different spe- cifications, estimation methods, and institutional variables. We also present evidence on the significance of institutions as a determinant of FDI over time. 1.},
	number = {3},
	journal = {Economics and Politics},
	author = {Daude, Christian and Stein, Ernesto},
	year = {2007},
	pages = {317--344}
}

